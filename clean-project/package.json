{
  "name": "clean-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "create-table": "ts-node --project tsconfig.scripts.json src/scripts/create-table.ts",
    "load-csv": "ts-node --project tsconfig.scripts.json src/scripts/load-csv-to-supabase.ts",
    "enrich-data": "ts-node --project tsconfig.scripts.json src/scripts/enrich-data.ts",
    "reprocess-failed": "ts-node --project tsconfig.scripts.json src/scripts/reprocess-failed.ts",
    "init-db": "ts-node --project tsconfig.scripts.json src/scripts/initialize-db.ts",
    "view-report": "ts-node --project tsconfig.scripts.json src/scripts/view-report.ts",
    "init-failed-table": "ts-node --project tsconfig.scripts.json -e \"import fs from 'fs'; import { execSync } from 'child_process'; const sql = fs.readFileSync('src/scripts/create-failed-enrichments-table.sql', 'utf8'); const cmd = `psql ${process.env.DATABASE_URL} -f src/scripts/create-failed-enrichments-table.sql`; try { execSync(cmd); console.log('Failed enrichments table created successfully'); } catch(e) { console.error('Error creating table:', e.message); }\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.1",
    "@types/uuid": "^10.0.0",
    "axios": "^1.8.3",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.7",
    "fs-extra": "^11.3.0",
    "next": "15.2.2",
    "p-limit": "^6.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  }
}
